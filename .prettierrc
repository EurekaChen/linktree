{
	"useTabs": false,
	"bracketSameLine": true,
	"semi": true,
	"tabWidth": 4,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 120,
	"htmlWhitespaceSensitivity": "ignore",
	"singleAttributePerLine": false,
	"proseWrap": "preserve",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
